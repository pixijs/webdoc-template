{
  "name": "@pixi/webdoc-template",
  "version": "1.3.3",
  "description": "PixiJS' webdoc template for API documentation",
  "main": "publish.js",
  "scripts": {
    "lint": "eslint *.js",
    "postversion": "git push && git push --tags",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/webdoc-template.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "pixi.js",
    "pixi",
    "webdoc",
    "documentation"
  ],
  "author": "Shukant K. Pal <shukantpal@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixijs/webdoc-template/issues"
  },
  "homepage": "https://github.com/pixijs/webdoc-template#readme",
  "files": [
    "publish.js",
    "helper.js",
    "static",
    "tmpl"
  ],
  "engines": {
    "node": ">= 12",
    "pnpm": "please-use-npm",
    "yarn": "please-use-npm",
    "npm": ">= 6"
  },
  "dependencies": {
    "code-prettify": "^0.1.0",
    "color-themes-for-google-code-prettify": "^2.0.4",
    "common-path-prefix": "^3.0.0",
    "fs-extra": "^9.0.1",
    "klaw-sync": "^6.0.0",
    "lodash": "*",
    "markdown-it": "^11.0.0",
    "markdown-it-highlightjs": "^3.4.0",
    "missionlog": "^1.6.0",
    "open-sans-fonts": "^1.6.2",
    "taffydb": "^2.7.3"
  },
  "peerDependencies": {
    "@webdoc/model": "^1.2.1",
    "@webdoc/template-library": "^1.2.1"
  },
  "devDependencies": {
    "clean-package": "^1.0.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "scripts",
      "engines",
      "pre-commit",
      "devDependencies"
    ]
  }
}
